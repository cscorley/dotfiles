Xcursor*theme: Jimmac
Xlock.mode: blank

urxvt.keysym.Control-Up:   \033[1;5A
urxvt.keysym.Control-Down:    \033[1;5B
urxvt.keysym.Control-Left:    \033[1;5D
urxvt.keysym.Control-Right:    \033[1;5C
urxvt*termName: rxvt-256color
!urxvt*termName: rxvt-unicode

urxvt*borderless: 1
urxvt.buffered: true
urxvt.underlineColor: yellow
urxvt.transparent:   true
urxvt.shading:       5
urxvt.background: rgba:3f00/3f00/3f00/dddd

!urxvt.font: xft:Terminus:pixelsize=15, xft:Terminus (TTF) for Powerline:pixelsize=15, xft:Lohit Kannada:pixelsize=15, xft:unifont:pixelsize=15
!urxvt.boldFont: xft:Terminus:Bold:pixelsize=15, xft:Terminus (TTF) for Powerline:BoldForPowerline:pixelsize=15, xft:Lohit Kannada:Bold:pixelsize=15, xft:unifont:pixelsize=15

!urxvt.font: xft:Meslo LG S DZ:Regular:size=10, xft:Meslo LG S DZ for Powerline:RegularForPowerline:size=10, xft:Lohit Kannada:Regular:size=10, xft:unifont:size=10
!urxvt.boldFont: xft:Meslo LG S DZ:Bold:size=10, xft:Meslo LG S DZ for Powerline:BoldForPowerline:size=10, xft:Lohit Kannada:Bold:size=10, xft:unifont:size=10
!urxvt.italicFont: xft:Meslo LG S DZ:Italic:size=10, xft:Meslo LG S DZ for Powerline:ItalicForPowerline:size=10, xft:Lohit Kannada:Italic:size=10, xft:unifont:pixelsize=10
!
urxvt.letterSpace: -1
!urxvt.lineSpace: 0

! Yep.
urxvt.font: xft:Menlo:Regular:size=10, xft:Menlo for Powerline:RegularForPowerline:size=10, xft:Lohit Kannada:Regular:size=10, xft:unifont:size=10
urxvt.boldFont: xft:Menlo:Bold:size=10, xft:Menlo for Powerline:BoldForPowerline:size=10, xft:Lohit Kannada:Bold:size=10, xft:unifont:size=10
urxvt.italicFont: xft:Menlo:Italic:size=10, xft:Menlo for Powerline:ItalicForPowerline:size=10, xft:Lohit Kannada:Italic:size=10, xft:unifont:pixelsize=10

!Plugin stuff
urxvt*urlLauncher:chromium
urxvt.perl-ext-common:default,matcher,clipboard,-option-popup,-selection-popup,-readline
urxvt.matcher.button:1

urxvt.tabbed.autohide:  yes
urxvt.tabbed.new-button:  no

urxvt*scrollBar: false
urxvt*saveLines: 10000
urxvt*secondaryScroll: true
urxvt*scrollTtyKeypress:true
urxvt*scrollTtyOutput:false
urxvt*scrollWithBuffer:false

urxvt*cursorBlink: true
urxvt*cursorColor: #FFAF00

! Molokai theme
urxvt*background: #101010
urxvt*foreground: #d0d0d0
!urxvt*cursorColor: #8000BA
urxvt*color0: #101010
urxvt*color1: #960050
urxvt*color2: #66aa11
urxvt*color3: #c47f2c
urxvt*color4: #30309b
urxvt*color5: #7e40a5
!urxvt*color6: #3579a8 
urxvt*color6: #89b6e2
urxvt*color7: #9999aa
urxvt*color8: #303030
urxvt*color9: #ff0090
urxvt*color10: #80ff00
urxvt*color11: #ffba68
urxvt*color12: #5f5fee
urxvt*color13: #bb88dd
urxvt*color14: #4eb4fa
urxvt*color15: #d0d0d0
