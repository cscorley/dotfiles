Xcursor*theme: Jimmac
Xlock.mode: blank

urxvt.keysym.Control-Up:   \033[1;5A
urxvt.keysym.Control-Down:    \033[1;5B
urxvt.keysym.Control-Left:    \033[1;5D
urxvt.keysym.Control-Right:    \033[1;5C
urxvt*termName: rxvt-256color
!urxvt*termName: rxvt-unicode

urxvt*foreground: #C7C7C7
urxvt*borderless: 1
urxvt.buffered: true
urxvt.underlineColor: yellow
urxvt.transparent:   true
urxvt.shading:       10
urxvt.background: rgba:3f00/3f00/3f00/dddd

urxvt.font: xft:Terminus:pixelsize=16, xft:Terminus (TTF) for Powerline:pixelsize=16, xft:Lohit Kannada:pixelsize=16, xft:unifont:pixelsize=16

urxvt.boldFont: xft:Terminus:Bold:pixelsize=16, xft:Terminus (TTF) for Powerline:BoldForPowerline:pixelsize=16, xft:Lohit Kannada:Bold:pixelsize=16

!Plugin stuff
urxvt*urlLauncher:chromium
urxvt.perl-ext-common:default,matcher,clipboard,-option-popup,-selection-popup,-readline
urxvt.matcher.button:1

urxvt.tabbed.autohide:  yes
urxvt.tabbed.new-button:  no

urxvt*scrollBar: false
urxvt*saveLines: 10000
urxvt*secondaryScroll: true
urxvt*scrollTtyKeypress:true
urxvt*scrollTtyOutput:false
urxvt*scrollWithBuffer:false

urxvt*cursorBlink: true
urxvt*cursorColor: #FFAF00

!Tango colors
!!Black + DarkGrey
urxvt*color0:#2e3436
urxvt*color8:#555753
!!DarkRed + Red
urxvt*color1:#a40000
urxvt*color9:#cc0000
!!DarkGreen + Green
urxvt*color2:#4e9a06
urxvt*color10:#73d216
!!DarkYellow + Yellow
urxvt*color3:#c4a000
urxvt*color11:#edd400
!!DarkBlue + Blue
urxvt*color4:#204a87
urxvt*color12:#3465a4
!!DarkMangenta + Mangenta
urxvt*color5:#ce5c00
urxvt*color13:#f57900
!!DarkCyan + Cyan ! (not tango)
urxvt*color6:#038e82
urxvt*color14:#05d2c1
!!LightGrey + White
urxvt*color7:#babdb6
urxvt*color15:#EEEEEC

